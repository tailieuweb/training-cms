{
    "name": "dhii/wp-containers",
    "description": "PSR-11 container implementations that wrap some WP features, for convenience and interoperability.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "container",
        "di",
        "dependency injection",
        "wordpress"
    ],
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0 | ^8.0",
        "psr/container": "^1.0",
        "dhii/data-container-interface": "^0.2.1-alpha1"
    },
    "require-dev": {
        "slevomat/coding-standard": "~4.0",
        "phpunit/phpunit": "^6",
        "gmazzap/andrew": "^1.1",
        "brain/monkey": "^2"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Wp\\Containers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\Wp\\Containers\\UnitTest\\": "test/unit",
            "Dhii\\Wp\\Containers\\FuncTest\\": "test/functional",
            "Dhii\\Wp\\Containers\\TestHelpers\\": "test/helpers"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}
