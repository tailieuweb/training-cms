{
    "name": "dhii/collections-interface",
    "description": "A highly ISP-compliant collection of interfaces that represent maps and lists.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        },
        {
            "name": "Anton Ukhanev",
            "email": "xedin.unknown@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1 | ^8.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0",
        "slevomat/coding-standard": "^6.0",
        "vimeo/psalm": "^3.11.7 | ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Collection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\Collection\\UnitTest\\": "tests/unit"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    }
}
